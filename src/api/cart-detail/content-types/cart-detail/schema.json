{
  "kind": "collectionType",
  "collectionName": "cart_details",
  "info": {
    "singularName": "cart-detail",
    "pluralName": "cart-details",
    "displayName": "CartDetail"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cart": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cart.cart"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "quantity": {
      "type": "integer",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    }
  }
}
